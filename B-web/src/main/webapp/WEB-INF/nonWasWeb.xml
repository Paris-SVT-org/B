<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app id="NonWasWebApp">
   <display-name>NonWasWebApp</display-name>
   <servlet>
      <servlet-name>IOBound</servlet-name>
      <display-name>IOBound</display-name>
      <servlet-class>com.ibm.wsmm.services.IOBound</servlet-class>
   </servlet>
   <servlet>
      <servlet-name>memBound</servlet-name>
      <display-name>memBound</display-name>
      <servlet-class>com.ibm.wsmm.services.memBound</servlet-class>
   </servlet>
   <servlet>
      <servlet-name>memBoundPerSession</servlet-name>
      <display-name>memBoundPerSession</display-name>
      <servlet-class>com.ibm.wsmm.services.memBoundPerSession</servlet-class>
   </servlet>
   <servlet>
      <servlet-name>CPUBound</servlet-name>
      <display-name>CPUBound</display-name>
      <servlet-class>com.ibm.wsmm.services.CPUBound</servlet-class>
   </servlet>
   <servlet>
      <servlet-name>sleepBound</servlet-name>
      <display-name>sleepBound</display-name>
      <servlet-class>com.ibm.wsmm.services.sleepBound</servlet-class>
   </servlet>
   <servlet>
      <servlet-name>simpleSleep</servlet-name>
      <display-name>simpleSleep</display-name>
      <servlet-class>com.ibm.wsmm.services.simpleSleep</servlet-class>
   </servlet>
   <servlet>
      <servlet-name>CpuAndSleepBound</servlet-name>
      <display-name>CpuAndSleepBound</display-name>
      <servlet-class>com.ibm.wsmm.services.CpuAndSleepBound</servlet-class>
   </servlet>
   <servlet-mapping>
      <servlet-name>IOBound</servlet-name>
      <url-pattern>/IOBound</url-pattern>
   </servlet-mapping>
   <servlet-mapping>
      <servlet-name>memBound</servlet-name>
      <url-pattern>/memBound</url-pattern>
   </servlet-mapping>
   <servlet-mapping>
      <servlet-name>memBoundPerSession</servlet-name>
      <url-pattern>/memBoundPerSession</url-pattern>
   </servlet-mapping>
   <servlet-mapping>
      <servlet-name>CPUBound</servlet-name>
      <url-pattern>/CPUBound</url-pattern>
   </servlet-mapping>
   <servlet-mapping>
      <servlet-name>sleepBound</servlet-name>
      <url-pattern>/sleepBound</url-pattern>
   </servlet-mapping>
   <servlet-mapping>
      <servlet-name>simpleSleep</servlet-name>
      <url-pattern>/simpleSleep</url-pattern>
   </servlet-mapping>
   <servlet-mapping>
      <servlet-name>CpuAndSleepBound</servlet-name>
      <url-pattern>/CpuAndSleepBound</url-pattern>
   </servlet-mapping>

   <welcome-file-list>
      <welcome-file>index.html</welcome-file>
      <welcome-file>index.htm</welcome-file>
      <welcome-file>index.jsp</welcome-file>
      <welcome-file>default.html</welcome-file>
      <welcome-file>default.htm</welcome-file>
      <welcome-file>default.jsp</welcome-file>
   </welcome-file-list>
   <error-page>
       <error-code>503</error-code>
       <location>/error.jsp</location>
   </error-page>
</web-app>
